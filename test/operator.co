O = ->
  switch ++i
  case 1 then {7}
  case 2 then new String 7
  default ok 0, 'returning delete should cache correctly'
eq delete (o = new O)[new O], 7
eq o[7], void

throws 'invalid deletion on line 1' -> Coco.compile 'delete a'
throws 'invalid deletion on line 1' -> Coco.compile 'delete 0'


