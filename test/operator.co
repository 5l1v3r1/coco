eq (10 |   3), 11
eq (10 ^   3), 9
eq (10 <<  3), 80
eq (10 >>  3), 1
eq (10 >>> 3), 1

num = 10; eq (num <<=  3), 80
num = 10; eq (num >>=  3), 1
num = 10; eq (num >>>= 3), 1
num = 10; eq (num &=   3), 2
num = 10; eq (num ^=   3), 9
num = 10; eq (num |=   3), 11


